        <control bouncinglimits="calc:image.cube ? true : false" />
        <view hlookat="1.211" vlookat="23.861" fovtype="MFOV" fov="120.000" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />
